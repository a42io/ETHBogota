{
    "private": true,
    "name": "backend",
    "version": "1.0.0",
    "scripts": {
        "dev": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register --respawn --files src/server.ts",
        "build": "tsc -p ./tsconfig.json && tsc-alias -p ./tsconfig.json",
        "type-check": "tsc --noEmit",
        "type-check:watch": "tsc --noEmit --watch",
        "eslint": "./node_modules/.bin/eslint 'src/**/*{ts,tsx}'",
        "eslint:fix": "./node_modules/.bin/eslint --fix 'src/**/*{ts,tsx}'"
    },
    "dependencies": {
        "@apollo/client": "^3.6.9",
        "@types/cors": "2.8.12",
        "@types/express": "4.17.14",
        "@types/luxon": "3.0.1",
        "@types/node": "18.7.18",
        "axios": "0.27.2",
        "circomlibjs": "^0.0.8",
        "cors": "2.8.5",
        "dotenv": "16.0.2",
        "ethers": "5.7.1",
        "express": "4.18.1",
        "firebase-admin": "11.0.1",
        "graphql": "^16.6.0",
        "jsonwebtoken": "8.5.1",
        "luxon": "3.0.3",
        "nanoid": "^4.0.0",
        "timezones-list": "^3.0.1",
        "ts-node-dev": "2.0.0",
        "typescript": "4.8.3",
        "wildcard-match": "5.1.2"
    },
    "devDependencies": {
        "tsc-alias": "^1.7.0",
        "tsconfig-paths": "^4.1.0"
    }
}
